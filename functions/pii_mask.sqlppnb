{"cells":[{"kind":2,"language":"SQL++","value":"SELECT default:ai_masked({\n\t\"text\": \"John Doe lives in New York. His email is john.doe@example.com.\",\n\t\"labels\": [\"person\", \"email\"],\n\t\"temperature\": 0.2,\n  \t\"max_tokens\": 600\n\t})"},{"kind":2,"language":"SQL++","value":"SELECT name,\n       content AS original_text,\n       default:ai_masked({\n           \"text\": content,\n           \"labels\": [\"person\", \"location\", \"email\", \"phone\", \"website\"],\n           \"temperature\": 0.2,\n           \"max_tokens\": 3000\n       }) AS masked_text\nFROM `Scripps`.`Notes`.`Patient`\nWHERE content LIKE '%@%' OR content LIKE '%contact%'\nLIMIT 1;"},{"kind":2,"language":"SQL++","value":"SELECT\n       p.visit_notes, p.doctor_id, p.doctor_name, p.patient_id, p.visit_date, p.visit_notes AS original_text,\n       default:ai_masked({\n           \"text\": p.visit_notes,\n           \"labels\": [\"person\", \"location\", \"email\", \"phone\", \"website\"],\n           \"temperature\": 0.2,\n           \"max_tokens\": 3000\n       }) AS masked_text\n       FROM `Scripps`.`Notes`.`Patient`\n       WHERE content LIKE '%@%' OR content LIKE '%contact%'\n       LIMIT 1;"}]}