CLUSTER_CONNECTION_STRING=couchbases://your-cluster.cloud.couchbase.com
CLUSTER_USERNAME=your-username
CLUSTER_PASS=your-password

# Main Healthcare Data Bucket (Scripps)
COUCHBASE_BUCKET=Scripps

# Capella Data API
DATA_API_ENDPOINT=https://your-cluster.data.cloud.couchbase.com

# Capella AI Services Model Service (for Vector Search Embeddings)
# 2048-dimensional embeddings for semantic search
EMBEDDING_MODEL_KEY=your-model-key
EMBEDDING_MODEL_TOKEN=cbsk-v1-your-token
EMBEDDING_MODEL_ENDPOINT=https://your-endpoint.ai.cloud.couchbase.com
EMBEDDING_MODEL_ID=your-model-id
EMBEDDING_MODEL_DIMENSIONS=2048
EMBEDDING_MODEL_REGION=us-your-region

# Vector Indexes for Hyperscale Search
# Research articles vectorized on article_vectorized field
HYPERSCALE_RESEARCH=hyperscale_pubmed_vectorized_article_vectorized
# Doctor notes vectorized on all_notes_vectorized field
HYPERSCALE_DOCTOR_NOTES=hyperscale_doctor_notes_vectorized_all_notes_vectorized

# Agent Catalog Setup (uses agent-catalog bucket)
AGENT_CATALOG_CONN_STRING=couchbases://your-cluster.cloud.couchbase.com
AGENT_CATALOG_USERNAME=your-username
AGENT_CATALOG_PASSWORD=your-password
AGENT_CATALOG_BUCKET=agent-catalog
AGENT_CATALOG_CATALOG=.agent-catalog
AGENT_CATALOG_ACTIVITY=.agent-activity
AGENT_CATALOG_DEBUG=false
AGENT_CATALOG_CONN_ROOT_CERTIFICATE=

# Open AI Model - LLM (for research summarization)
OPENAI_API_KEY=sk-...

# LangSmith tracing (optional)
LANGSMITH_API_KEY=
